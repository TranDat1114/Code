<h1 class="h2 my-3">@ViewBag.Title</h1>
<div>
    <table class="table">
        <thead>
            <tr>
                <th scope="col">#</th>
                <th scope="col">Name</th>
                <th scope="col">Price</th>
                <th scope="col">Quantity</th>
            </tr>
        </thead>
        <tbody>
            @foreach(var item in (List<Product>)ViewBag.Products)
            {
                <tr>
                    <th scope="row">@(item.Id) </th>
                    <td>@(item.Name)</td>
                    <td>@(item.Price)</td>
                    <td>@(item.Quantity)</td>
                </tr>
            }
        </tbody>
    </table>
</div>